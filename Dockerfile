#skylar 6 based on docker ubuntuFROM 		ubuntu:14.04MAINTAINER  Ming RUN apt-get update#install wget curlRUN apt-get install -y wgetRUN apt-get install -y curl#install redisRUN apt-get install -y redis-server#install nginx#libz for luajitRUN ln -s /lib/x86_64-linux-gnu/libz.so.1 /lib/x86_64-linux-gnu/libz.soCOPY opt/openresty /opt/openrestyRUN chown -R cloud:cloud /opt/openresty/# tell Nginx to stay foregroundedRUN echo "daemon off;" >> /opt/openresty/nginx/conf/nginx.conf#install supervisorRUN apt-get install -y supervisorEXPOSE 80CMD ["/usr/bin/supervisord"]